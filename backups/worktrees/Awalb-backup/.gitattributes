# Prevent accidentally including private premium assets in source tarballs
NomadMuse/models/** export-ignore
NomadAssets/** export-ignore

# Treat model artefacts as binary and recommend LFS if added
*.onnx binary
*.pt binary
*.pth binary
*.h5 binary
*.ckpt binary
*.bin binary

# Ensure line endings consistent for source
*.cpp text eol=lf
*.h text eol=lf
*.md text eol=lf
